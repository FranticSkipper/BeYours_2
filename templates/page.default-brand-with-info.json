    "3cc0137c-9505-44c4-a49f-f24a6c9f4a1e": {
      "type": "image-with-text",
      "blocks": {
        "template--15814370132015__3cc0137c-9505-44c4-a49f-f24a6c9f4a1e-heading-1": {
          "type": "heading",
          "settings": {
            "heading": "PROFESSIONAL TREATMENTS AND HOME CARE",
            "heading_size": "h1",
