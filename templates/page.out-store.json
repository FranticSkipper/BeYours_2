    "c01c5217-4eb0-4578-89ca-ebfd0d70a183": {
      "type": "rich-text",
      "blocks": {
        "template--16061252042799__c01c5217-4eb0-4578-89ca-ebfd0d70a183-heading-1": {
          "type": "heading",
          "settings": {
            "heading": "I want to change the way",
            "heading_size": "h1",
            "heading_tag": "h2"
          }
        },
        "template--16061252042799__c01c5217-4eb0-4578-89ca-ebfd0d70a183-text-1": {
          "type": "text",
          "settings": {
            "text": "<p>We are exiting the era of marketing-based skincare and entering the era of performance-based skincare. skincare needs to embrace the individuality and diverse skincare needs of every consumer<\/p>",
            "text_size": "typeset2",
            "secondary_color": false,
            "highlight_phrases": ""
          }
        }
      },
